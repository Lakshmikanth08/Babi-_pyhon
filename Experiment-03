import math
def ball_collide(x1,y1,r1,x2,y2,r2)
dist=math.sqrt((x2-x1)**2+( y2-y1)**2)
print("distance between the two ball:",dist)
r=r1+r2
print("sum of radii:",r)
if dist<=r:
print("they are colliding")
return true
else:
print("not colliding")
return false 
c1=ball_collide(4,4,3,2,2,3)
print(c1)
c12=ball_collide(100,200,20,200,100,10)
print(c2)
